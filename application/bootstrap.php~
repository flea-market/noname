<?php
use lib\completed_classes\router;
use lib\completed_classes\registry;
function myAutoloader($class){
    $class = str_replace('\\','/',$class).'.php';
    echo $class.'<br>';
    require_once($class);
}
spl_autoload_register('myAutoloader');
router::start();
// registry::instance();
$a = registry::instance();
$a::setData('qwerty','qwerty value');
echo $a::getData('qwerty');
echo "test";
$a::setData('qwerty','asjdgskdfjhgsf');
echo $a::getData('qwerty');
?>